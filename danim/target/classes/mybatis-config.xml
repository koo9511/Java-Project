    </settings>
	<typeAliases>
		<typeAlias type="com.kh.danim.user.model.vo.User" alias="User" />
		<typeAlias type="com.kh.danim.user.model.vo.Chatting" alias="Chatting" />
		<typeAlias type="com.kh.danim.notice.model.vo.Notice" alias="Notice" />
