		<typeAlias type="com.kh.danim.help.model.vo.Help" alias="Help" />
		<typeAlias type="com.kh.danim.admin.model.vo.Admin" alias="Admin" />
		
	</typeAliases>
	  
	<mappers>
		<mapper resource="mappers/user-mapper.xml" />
